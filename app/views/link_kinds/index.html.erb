<% content_for :title, "Link kinds" %>

<h1>Link kinds</h1>

<div id="link_kinds">
  <% @link_kinds.each do |link_kind| %>
    <%= render link_kind %>
    <p>
      <%= link_to "Show this link kind", link_kind %>
    </p>
  <% end %>
</div>

<%= link_to "New link kind", new_link_kind_path %>
